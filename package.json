{
  "name": "vitae",
  "private": true,
  "description": "Personal CV",
  "author": "Eirik Albrigtsen <analsandblaster@gmail.com>",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "clux/vitae"
  },
  "scripts": {
    "test": "npm run build",
    "build": "src/build.js > vitae.html",
    "release": "wkhtmltopdf vitae.html eirik.albrigtsen.cv-$(date +%Y-%m-%d).pdf",
    "watch": "watchy -w 'src/*' npm run build",
    "sync": "browser-sync start --server --files=vitae.html --startPath=/vitae.html --reload-delay=100",
    "develop": "npm run build && parallelshell 'npm run watch' 'npm run sync'"
  },
  "dependencies": {
    "combustion": "~0.3.1"
  },
  "devDependencies": {
    "browser-sync": "^2.8.2",
    "parallelshell": "^2.0.0",
    "watchy": "^0.6.6"
  },
  "license": "MIT"
}
